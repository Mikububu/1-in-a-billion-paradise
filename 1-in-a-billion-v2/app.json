{
  "expo": {
    "name": "1 In A Billion V2",
    "slug": "in-a-billion-v2",
    "scheme": "oneinabillion",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oneinabillion.app.v2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.oneinabillion.app.v2",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-font",
      "expo-apple-authentication",
      "expo-image-picker",
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "30ea6fa7-5f80-4f98-a4d2-bf4909694f48"
      }
    },
    "owner": "michaelpw"
  }
}
