{
  "expo": {
    "name": "1 In A Billion V2",
    "slug": "in-a-billion-v2",
    "scheme": "oneinabillionv2",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oneinabillion.app.v2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.oneinabillion.app.v2",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      "expo-font",
      "expo-apple-authentication",
      "expo-image-picker",
      "expo-notifications"
    ]
  }
}
